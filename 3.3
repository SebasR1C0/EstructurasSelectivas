#include <iostream>
#include <conio.h>
using namespace std;
int main() {
	int a,c,d,u;
	cout << "Inserte su numero de 3 cifras " << endl;
	cin >> a;
	u = a % 10;
	c = a / 10;
	d = c % 10;
	c /= 10;
	if (a <= 999 && a >= 100) {
		switch (c) {
		case 1:case 2:case 3: cout << "X"; break;
		case 4:case 5:case 6: cout << "Y"; break;
		case 7:case 8:case 9: cout << "Z"; break;
		default: cout << "#";
		}
